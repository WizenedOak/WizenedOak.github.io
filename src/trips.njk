---
title: My Trips
layout: layout.njk
---

<section class="trips_card_container">

  {% for trip in trips %}
    <div class="trips_card">
      <img src="{{ trip.img }}" alt="{{ trip.alt }}" loading="lazy">
      <div class="trips_card_content">
        <h3>{{ trip.title }}</h3>
        <p>{{ trip.description }}</p>
      </div>
    </div>
  {% endfor %}

  </div>
</section>